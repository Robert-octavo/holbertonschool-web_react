{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production --config config/webpack.config.js",
    "test": "jest",
    "start": "webpack server --open --mode development --config config/webpack.config.js",
    "test-watch": "jest --watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^9.1.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "webpack": "^5.82.0",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^11.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.1",
    "image-webpack-loader": "^8.1.0",
    "inline-source-map": "^0.6.2",
    "jest": "^27.5.1",
    "jest-transform-stub": "^2.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-test-renderer": "^17.0.1",
    "style-loader": "^3.3.2",
    "web-vitals": "^2.1.4"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css|sass)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(jpg|jpeg|png|gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js",
      "/^Footer\/(.*)$/": "<rootDir>/src/Footer/$1",
      "/^Header\/(.*)$/": "<rootDir>/src/Header/$1",
      "/^App\/(.*)$/": "<rootDir>/src/App/$1",
      "/^Login\/(.*)$/": "<rootDir>/src/Login/$1",
      "/^Notifications\/(.*)$/": "<rootDir>/src/Notifications/$1",
      "/^utils\/(.*)$/": "<rootDir>/src/utils/$1"
    }
  },
  "moduleFileExtensions": [
    "js",
    "json",
    "vue",
    "scss",
    "css"
  ]
}
